<p id="notice"><%= notice %></p>

<div class="row">
  <div class="large-12 columns">

  <div class="left">
    <h1>Counties</h1>
  </div>

  </div>
</div>

<div class="row">
<div class="large-12 columns">

<div class="panel">

<table border= "1px solid black">

  <thead>
      <tr>
        <th>County</th>
        <th>Coordinator</th>
        <th>Contact</th>
        <th>Edit</th>
      </tr>
    </thead>

  <tbody>
    <% @counties.each do |county| %>
      <tr>
        <td><%= county.name%></td>
        <td><%= county.coordinator%></td>
        <td>
          <%= county.phone%>
          <br></br>
          <%= link_to "#{county.website}", "#{county.website}", :target => "_blank" %>
        </td>
        <td><%= link_to 'Edit', edit_county_path(county) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

</div></div>
</div>

