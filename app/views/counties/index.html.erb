<p id="notice"><%= notice %></p>

<h1>Counties</h1>

<div class="row">
<div class="large-12 columns">
<div class="panel">

<table border= "1px solid black">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @counties.each do |county| %>
      <tr>
        <td><%= link_to county.name, county %></td>
        <td><%= link_to 'Edit', edit_county_path(county) %></td>
        <td><%= link_to 'Delete', county, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'New County', new_county_path %>

</div></div>
</div>

<br>
